<ejb-jar>
	<enterprise-beans>
		<session>
			<ejb-name>CustomerBean</ejb-name>
			<ejb-class>com.example.ejb.session.CustomerBean</ejb-class>
			<local>com.example.ejb.session.CustomerBean</local>
			<!--  <remote></remote> -->
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<env-entry>
				<env-entry-name>customerTitle</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>Sr.</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>