CREATE TABLE t_track (TRACKID BIGINT NOT NULL, t_description VARCHAR(255), t_duration FLOAT, t_title VARCHAR(50), WAV LONGBLOB, track_id_fk BIGINT, PRIMARY KEY (TRACKID))
CREATE TABLE CD (CDID BIGINT NOT NULL, DESCRIPTION VARCHAR(255), PRICE DOUBLE, TITLE VARCHAR(255), PRIMARY KEY (CDID))
CREATE TABLE STAFFMEMBER (CODE VARCHAR(255) NOT NULL, INITIALS VARCHAR(255) NOT NULL, LASTNAME VARCHAR(255), NAME VARCHAR(255), PRIMARY KEY (CODE, INITIALS))
CREATE TABLE NEWSLETTER (NEWSLETTERID BIGINT NOT NULL, NEWSLETTERDATE DATE, PRIMARY KEY (NEWSLETTERID))
CREATE TABLE NEWS (CONTENT VARCHAR(255), LANGUAGE VARCHAR(255) NOT NULL, TITLE VARCHAR(255) NOT NULL, NEWSLETTERID_FK BIGINT, PRIMARY KEY (LANGUAGE, TITLE))
CREATE TABLE ITEM (ITEMID BIGINT NOT NULL, itemType VARCHAR(31), ITEM VARCHAR(255), PRICE DECIMAL(38), QUANTITY INTEGER, COVER LONGBLOB, DESCRIPTION VARCHAR(255), TITLE VARCHAR(255), CURRENCY VARCHAR(255), ISBN VARCHAR(255), PRIMARY KEY (ITEMID))
CREATE TABLE ORDERITEM (ORDERITEMID BIGINT NOT NULL, ITEM VARCHAR(255), QUANTITY INTEGER, UNITPRICE DOUBLE, PRIMARY KEY (ORDERITEMID))
CREATE TABLE t_order (ORDERID BIGINT NOT NULL, CREATIONDATE DATETIME, PRIMARY KEY (ORDERID))
CREATE TABLE ADDRESS (ADDRESSID BIGINT AUTO_INCREMENT NOT NULL, STREET1 VARCHAR(20), STREET2 VARCHAR(20), PRIMARY KEY (ADDRESSID))
CREATE TABLE city (ADDRESSID BIGINT NOT NULL, CITY VARCHAR(255), STATE VARCHAR(255), ZIPCODE VARCHAR(255), PRIMARY KEY (ADDRESSID))
CREATE TABLE country (ADDRESSID BIGINT NOT NULL, COUNTRY VARCHAR(255), COUNTRYCODE VARCHAR(255), PRIMARY KEY (ADDRESSID))
CREATE TABLE CUSTOMER (CUSTOMERID BIGINT NOT NULL, CREATIONDATE DATETIME, DATEOFBIRTH DATE, FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), EMAIL VARCHAR(255), LINKEDINACCOUNT VARCHAR(255), PHONENUMBER VARCHAR(255), BILLADDRESS_FK BIGINT, PRIMARY KEY (CUSTOMERID))
CREATE TABLE COMMENT (ID BIGINT NOT NULL, CONTENT VARCHAR(255), POSTEDDATE DATETIME, RATE INTEGER, PRIMARY KEY (ID))
CREATE TABLE ARTIST (ARTISTID BIGINT NOT NULL, FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), PRIMARY KEY (ARTISTID))
CREATE TABLE NEWS_COMMENT (LANGUAGE VARCHAR(255) NOT NULL, TITLE VARCHAR(255) NOT NULL, comments_ID BIGINT NOT NULL, PRIMARY KEY (LANGUAGE, TITLE, comments_ID))
CREATE TABLE Album_TRACKS (Album_ITEMID BIGINT, title VARCHAR(255), position INTEGER)
CREATE TABLE join_order_item (order_fk BIGINT NOT NULL, order_line_fk BIGINT NOT NULL, PRIMARY KEY (order_fk, order_line_fk))
CREATE TABLE join_artist_cd (artist_fk BIGINT NOT NULL, cd_fk BIGINT NOT NULL, PRIMARY KEY (artist_fk, cd_fk))
CREATE TABLE Tag (Book_ITEMID BIGINT, Value VARCHAR(255))
