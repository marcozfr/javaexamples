<?xml version="1.0" encoding="UTF-8"?>
<jaxws:bindings wsdl="../wsdl/LongProcessService.wsdl"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	jaxws:schemaLocation="http://java.sun.com/xml/ns/jaxws wsdl_customizationschema_2_0.xsd">
	<!-- <jaxws:enableWrapperStyle>true</jaxws:enableWrapperStyle> -->
	<!-- <jaxws:enableAsyncMapping>true</jaxws:enableAsyncMapping> -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='LongProcessServicePort']">
		<jaxws:class name="LongProcessService" />
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='LongProcessServicePort']/wsdl:operation[@name='runProcess']">
		<jaxws:method name="run" />
		<jaxws:parameter part="wsdl:definitions/wsdl:message[@name='runProcessRequest']/wsdl:part[@name='parameters']"
                 childElementName="tns:runProcess" name="processId"/>
        <jaxws:parameter part="wsdl:definitions/wsdl:message[@name='runProcessResponse']/wsdl:part[@name='parameters']"
                 childElementName="tns:runProcess" name="processStatus"/>
	</jaxws:bindings>
	<jaxws:provider />
</jaxws:bindings>